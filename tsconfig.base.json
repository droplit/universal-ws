{
    "compilerOptions": {
        "allowUnreachableCode": false,
        "declaration": true,
        "declarationMap": true,
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "preserveConstEnums": true,
        "target": "es2018",
        "module": "commonjs",
        "inlineSourceMap": true,
        "strict": true,
        "pretty": true,
        "types": [
            "node"
        ]
    }
}